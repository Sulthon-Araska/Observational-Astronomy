100)\n","  JD2000 = int(365.25*(Y+4716)) + int(30.6001*(M+1)) + D + (2 - A + int(A/4)) - 1524.5\n","  ET = JD - JD2000  #Nilai fraksi (per 24 jam)\n","  #Konversi ET ke UT (dalam detik)\n","  UT = 24*3600*ET - ΔT\n","  UT = str(datetime.timedelta(seconds = UT)) #Nilai fraksi dalam jam-menit-detik\n","  return UT"],"metadata":{"id":"Mt6olQJ6NJnI","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1668442195435,"user_tz":-420,"elapsed":29,"user":{"displayName":"Sulthon Furqandhani Araska","userId":"13263034908675274881"}},"outputId":"c0d77b5f-07e1-4ee7-ccec-565772e9efc1"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["ΔT = 27.14989916051096 s\n"]}]},{"cell_type":"code","source":["#@title Durasi Gerhana Bulan\n","n = 0.5458 + 0.0400*cos(Mm)\n","h = 1.5573 + u  #Penumbral\n","p = 1.0128 - u  #Parsial\n","t = 0.4678 - u  #Total\n","\n","#Paruh Durasi (semiduration)\n","Half_Penumbral = (60/n)*np.sqrt(abs(h**2 - γ**2))\n","Half_Partial   = (60/n)*np.sqrt(abs(p**2 - γ**2))\n","Half_Umbral    = (60/n)*np.sqrt(abs(t**2 - γ**2))\n","\n","#Durasi Gerhana (dalam menit)\n","Penumbral = 2*Half_Penumbral\n","Partial   = 2*Half_Partial\n","Umbral    = 2*Half_Umbral\n","Durasi    = Penumbral + Partial + Umbral\n","#Durasi Gerhana (dalam jam-menit-detik)\n","Penumbral = str(datetime.timedelta(minutes = Penumbral))\n","Partial   = str(datetime.timedelta(minutes = Partial))\n","Umbral    = str(datetime.timedelta(minutes = Umbral))\n","Durasi    = str(datetime.timedelta(minutes = Durasi))\n","\n","#Tampilkan hasil:\n","print('Gerhana bulan berlangsung selama', Durasi, 'dengan rincian:')\n","print('Durasi gerhana penumbra:', Penumbral)\n","print('Durasi gerhana parsial :', Partial)\n","print('Durasi gerhana total   :', Umbral)"],"metadata":{"id":"ZsiKBskUC1-W","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1668442195436,"user_tz":-420,"elapsed":27,"user":{"displayName":"S